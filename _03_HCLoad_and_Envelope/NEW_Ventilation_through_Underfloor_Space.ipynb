   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "def get_U_s_vert(Region, Q):\n",
    "    if Region == 'Region8':\n",
    "        return 2.223\n",
    "    else:\n",
    "        if Q > Q_j[Region][1]:\n",
    "            return min(2.223, ( Q - Q_j[Region][1] ) / ( Q_j[Region][0] - Q_j[Region][1]) * U_s_R_Q_j[Region][0] + ( Q - Q_j[Region][0] ) / ( Q_j[Region][1] - Q_j[Region][0]) * U_s_R_Q_j[Region][1] )\n",
    "        elif Q > Q_j[Region][2]:\n",
    "            return min(2.223, ( Q - Q_j[Region][2] ) / ( Q_j[Region][1] - Q_j[Region][2]) * U_s_R_Q_j[Region][1] + ( Q - Q_j[Region][1] ) / ( Q_j[Region][2] - Q_j[Region][1]) * U_s_R_Q_j[Region][2] )\n",
    "        elif Q > Q_j[Region][3]:\n",
    "            return min(2.223, ( Q - Q_j[Region][3] ) / ( Q_j[Region][2] - Q_j[Region][3]) * U_s_R_Q_j[Region][2] + ( Q - Q_j[Region][2] ) / ( Q_j[Region][3] - Q_j[Region][2]) * U_s_R_Q_j[Region][3] )\n",
    "        else:\n",
    "            return min(2.223, ( Q - Q_j[Region][4] ) / ( Q_j[Region][3] - Q_j[Region][4]) * U_s_R_Q_j[Region][3] + ( Q - Q_j[Region][3] ) / ( Q_j[Region][4] - Q_j[Region][3]) * U_s_R_Q_j[Region][4] )"
   ]
  },
