  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "U_s_R_Q_j = {\n",
    "    'Region1' : [2.223, 0.342, 0.427, 0.319, 0.272],\n",
    "    'Region2' : [2.223, 0.342, 0.427, 0.319, 0.272],\n",
